{
  "architecture": {
    "hidden_channels": 64,
    "lifting_channels": 128,
    "projection_channels": 256,
    "n_layers": 2,
    "n_modes": [
      16,
      16
    ],
    "in_channels": 1,
    "dropout": 0.3,
    "n_attn_heads": 2,
    "fno_output_layer_indices": [
      0,
      1
    ]
  },
  "probabilistic": {
    "n_samples": 400,
    "monte_carlo_samples": 200,
    "confidence_threshold": 0.3,
    "uncertainty_threshold": 0.2,
    "numerical_stability_check": true
  },
  "causal": {
    "use_dag_constraint": true,
    "dag_constraint_method": "improved"
  },
  "training": {
    "num_epochs": 50,
    "batch_size": 200,
    "learning_rate": 0.0006,
    "optimizer": "AdamW",
    "weight_decay": 0,
    "gradient_clipping": 1.0,
    "scheduler": {
      "name": "CosineAnnealingWarmRestarts",
      "T_0": 15,
      "T_mult": 3,
      "eta_min": 2e-05
    },
    "patience": 7,
    "min_delta": 5e-05,
    "early_stopping_thresh": 1.0,
    "init_iter": 10,
    "loss_weights": {
      "recon_loss": 5,
      "reconstruction_weights": {
        "dist": 0.6,
        "direct": 0.4
      },
      "l1_regularization": 0.0005,
      "l2_regularization": 0.0015,
      "entropy_regularization": 0.05,
      "dag_constraint": 0.001
    }
  },
  "data": {
    "file_path": "data/generated_time_series2.csv",
    "ground_truth_file": "data/causal_adjacency_matrix2.csv",
    "sequence_length": 50,
    "train_ratio": 0.8,
    "max_time_steps": 1000,
    "preprocessing": {
      "method": "standard",
      "standard": {
        "normalize": true,
        "normalization_method": "zscore",
        "handle_missing": true,
        "missing_method": "interpolate",
        "handle_outliers": true,
        "outlier_method": "iqr",
        "outlier_threshold": 3.0,
        "clip_outliers": true,
        "clip_percentiles": [
          1,
          99
        ]
      }
    },
    "augment": false,
    "augmentation": {
      "factor": 5,
      "techniques": [
        "scaling"
      ],
      "noise_level": [
        0.01,
        0.05
      ],
      "scale_range": [
        0.8,
        1.2
      ]
    }
  },
  "evaluation": {
    "auto_threshold_selection": true,
    "threshold_range": [
      0.1,
      0.9
    ],
    "threshold_steps": 10,
    "metrics": [
      "f1",
      "precision",
      "recall",
      "tpr",
      "fpr",
      "shd"
    ],
    "eval_samples": 100
  },
  "random_seed": {
    "use_fixed_seed": true,
    "fixed_seed": 21,
    "save_seed": true,
    "seed_file": "experiment_seed.txt"
  },
  "joint_learning": {
    "enabled": false,
    "method": "hybrid",
    "hidden_dim": 64
  },
  "probability_scaling": {
    "enable_log_scaling": true,
    "enforce_dag_constraint": true,
    "tiny_threshold": 0.25
  },
  "debug_visualization": {
    "debug_mode": false,
    "verbose_logging": true,
    "save_intermediate_plots": false,
    "plot_frequency": 10,
    "tensorboard_logging": false
  },
  "runtime": {
    "actual_seed": 21
  }
}